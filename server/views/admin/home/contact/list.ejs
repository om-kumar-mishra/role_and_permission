<%- include('../../../partials/header'); %>
    <%- include('../../../partials/sidebar'); %>
        <div class="content-wrapper">
            <%- include("../../../notify"); %> 
            <%- toasts %> 

            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1 class="m-0">Contact List</h1>
                        </div>
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
                                <li class="breadcrumb-item active">Contact List</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
           
            <section class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <!-- <h1>Course Categories</h1> -->
                        </div>
                        <div class="col-sm-6">
                            <% if(list.length < 1) {%>
                                <a href="/contact" class="btn btn-primary">Add</a>
                                <% }%>
                            <ol class="breadcrumb float-sm-right">
                             
                            </ol>
                        </div>
                    </div>
                </div>
            </section>
            
           
            <section class="content">
                <div class="container-fluid">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">Contact List</h3>
                        </div>

                        <div class="card-body">
                            <table id="example1" class="table table-bordered table-striped">
                                <thead>
                                    <tr>
                                        <th>Sr No</th>
                                        <th>Address</th>
                                        <th>Email</th>
                                        <th>Second Email</th>
                                        <th>Phone number</th>
                                        <th>Second Phone number</th>
                                        <th>Facebook</th>
                                        <th>Instagram</th>
                                        <th>LinkedIn</th>
                                        <th>Map</th>
                                        <th>Summary</th>
                                       
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if(list.length){ list.forEach((element,index)=>{%>
                                 
                                        <tr>
                                            <td>
                                                <%= index+1 %>
                                            </td>
                                            <td>
                                                <%= element.address %>
                                            </td>
                                            <td>
                                                <%= element.email %>
                                            </td>
                                            <td>
                                                <%= element.email2 %>
                                            </td>
                                            <td>
                                                <%- element.phone %> 
                                            </td>
                                            <td>
                                                <%- element.phone2 %> 
                                            </td>
                                        
                                            <td>
                                                <%- element.facebook %>                                             
                                            </td>

                                            <td>
                                                <%- element.instagram %>                                             
                                            </td>

                                            <td>
                                                <%- element.linkedIn %>                                             
                                            </td>

                                            <td>
                                                <%- element.map%>                                             
                                            </td>
                                            <td>
                                                <%- element.summary %>                                             
                                            </td>
                                            <td>
                                                <a href="/delete-contact?contactId=<%= element.id %>" ><i class="fa fa-trash-o" style="font-size:25px; color:red"></i></a>
                
                                                <a href="/update-contact?contactId=<%= element.id %>" ><i class='fas fa-pen-nib' style="font-size:25px;"></i></a>
                                             </td>
                                            
                                           
                                        </tr>
                                        <%  })  } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </section>
        </div>
        <%- include('../../../partials/footer'); %>
      
        

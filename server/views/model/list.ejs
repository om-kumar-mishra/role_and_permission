<%- include('../partials/header'); %>
    <%- include('../partials/sidebar'); %>
        <div class="content-wrapper">
            <%- include("../notify"); %> 
            <%- toasts %>

            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1 class="m-0">Role</h1>
                        </div>
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
                                <li class="breadcrumb-item active">Collection of model</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
           
            <section class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <!-- <h1>Course Categories</h1> -->
                        </div>
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                             
                                    <a href="/add-model" class="btn btn-primary">Add</a>
                               
                            </ol>
                        </div>
                    </div>
                </div>
            </section>
            
           
            <section class="content">
                <div class="container-fluid">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">Collection of model</h3>
                        </div>

                        <div class="card-body">
                            <table id="example1" class="table table-bordered table-striped">
                                <thead>
                                    <tr>
                                        <th>Sr No</th>
                                        <th>Name</th>
                                                         
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if(list.length){ list.forEach((element,index)=>{%>
                                 
                                        <tr>
                                            <td>
                                                <%= index+1 %>
                                            </td>
                                            <td>
                                                <%- element.Name %>
                                            </td>
                                          
                                           
                                            <td>
                                                <a href="/delete-model?id=<%= element.id %>" ><i class="fa fa-trash-o" style="font-size:25px; color:red"></i></a>
                
                                                <a href="/update-model?id=<%= element.id %>" ><i class='fas fa-pen-nib' style="font-size:25px;"></i></a>
<!-- 
                                                <a href="/get-permission-page?id=<%= element.id %>" >ASSIGN PERMISSION</a> -->
                                             </td>
                                            
                                           
                                        </tr>
                                        <%  })  } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </section>
        </div>
        <%- include('../partials/footer'); %>
      
        
